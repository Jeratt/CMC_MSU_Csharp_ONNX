<Window x:Class="YOLO_View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YOLO_View"
        mc:Ignorable="d"
        Title="MainWindow" x:Name="MyWindow" Height="450" Width="800">
    <Grid>
        <Image x:Name="pic" HorizontalAlignment="Left" Margin="51,27,0,63" Width="628" Height="400"/>
        <Menu Grid.ColumnSpan="2">
            <MenuItem Cursor="AppStarting" Header="File" Click="MenuItem_Click">
                <MenuItem Header="Load" Command="{x:Static local:MainWindow.LoadImageFromFileCommand}"/>
            </MenuItem>

        </Menu>
    </Grid>
</Window>
